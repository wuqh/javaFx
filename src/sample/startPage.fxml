<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>

        <Label layoutX="159.0" layoutY="50.0" prefWidth="355.0" text="Group 4 Grouping Tool" textFill="black">
            <font>
                <Font name="System Bold Italic" size="29.0" />
            </font>
        </Label>
        <Label layoutX="200.0" layoutY="155.0" text="Number of Students:" textFill="black">
            <font>
                <Font name="System Bold Italic" size="20.0" />
