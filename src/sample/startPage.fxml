        </Label>
        <Label layoutX="106.0" layoutY="250.0" text="Press To Enter Students" textFill="black">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Label>
        <JFXButton fx:id="btn_add" buttonType="RAISED" layoutX="120.0" layoutY="300.0" mnemonicParsing="false" onAction="#onAdd" prefHeight="15.0" prefWidth="100.0" text="Add Student" />

        <JFXButton fx:id="btn_start" buttonType="RAISED" layoutX="250.0" layoutY="300.0" mnemonicParsing="false" onAction="#onStart" prefHeight="15.0" prefWidth="100.0" text="Start" />
